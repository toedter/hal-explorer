    "@angular/router": "^21.0.6",
    "bootstrap": "^5.3.3",
    "core-js": "^3.41.0",
    "http-status-codes": "^2.3.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "uri-templates": "^0.2.0",
    "webdriver-manager": "^12.1.9",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "^21.0.3",
